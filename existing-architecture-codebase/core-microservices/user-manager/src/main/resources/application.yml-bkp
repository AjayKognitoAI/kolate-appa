server:
  port: 9030

spring:
  application:
    name: user-manager
  jackson:
    property-naming-strategy: SNAKE_CASE
  main:
    allow-bean-definition-overriding: true

feign:
  clients:
    enterprise-manager:
      path: /internal/postgres-database-manager

eureka:
  client:
    service-url:
      defaultZone: http://localhost:8761/eureka
    fetch-registry: true
    register-with-eureka: true
  instance:
    prefer-ip-address: true
