# Development Environment (Docker)
# This file is used when running with Docker Compose

# Application Configuration
APP_NAME=copilot-data-science-ml-agent
VERSION=2.0.0
ENVIRONMENT=dev
HOST=0.0.0.0
PORT=8000

# Eureka Configuration (Docker network)
EUREKA_SERVER=http://service-registry:8761/eureka
EUREKA_INSTANCE_HOST=copilot-data-science-ml-backend

# Google AI Configuration
GOOGLE_API_KEY=
MODEL_NAME=gemini-2.0-flash-exp

# Azure Configuration (optional)
AZURE_API_KEY=
AZURE_API_BASE=
AZURE_API_VERSION=2024-02-01

# Database Configuration (SQLite in container)
DATABASE_URL=sqlite:////app/data/db/data_science_agent.db

# ChromaDB Configuration
CHROMA_DB_DIR=/app/data/chroma_db

# Application Settings
BACKEND_URL=http://copilot-data-science-ml-backend:8000
MAX_ITERATIONS=10
TEMPERATURE=0.7
UPLOAD_DIR=/app/data/uploads
MAX_FILE_SIZE_MB=100
SESSION_TIMEOUT_MINUTES=60

# Logging
LOG_LEVEL=info
