# Google AI Configuration
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_PROJECT_ID=your_project_id
# GOOGLE_LOCATION=us-central1

 # Azure OpenAI Configuration
MODEL_NAME=gpt-4o
AZURE_API_KEY=xyz
AZURE_API_BASE=xyz
AZURE_API_VERSION=2024-02-01

# Database Configuration
# DATABASE_URL=postgresql://user:password@localhost:5432/data_science_agent
# Or for SQLite (simpler for getting started):
DATABASE_URL=sqlite:///./data_science_agent.db

# Agent Configuration
MAX_ITERATIONS=10
TEMPERATURE=0.7
MODEL_NAME=gemini-2.0-flash-exp

# File Upload Configuration
UPLOAD_DIR=./data/uploads
MAX_FILE_SIZE_MB=100

# Session Configuration
SESSION_TIMEOUT_MINUTES=60

# Backend URL (for frontend to access visualizations)
BACKEND_URL=http://localhost:8000

# ChromaDB Configuration
CHROMA_DB_DIR=./data/chroma_db

# AWS S3 Configuration (for trial-based data sync)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1
# Optional: For LocalStack/MinIO testing
# AWS_S3_ENDPOINT_URL=http://localhost:4566

# S3 Trial Configuration
S3_BUCKET_NAME=your-bucket-name
S3_BASE_PATH=trials/
S3_TEMP_DIR=./data/s3_temp

# S3 Sync Scheduler Configuration
S3_SYNC_ENABLED=true
S3_POLL_INTERVAL_MINUTES=15
