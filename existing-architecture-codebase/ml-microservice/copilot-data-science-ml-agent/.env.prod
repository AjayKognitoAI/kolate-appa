# Production Environment
# This file is used for production deployment

# Application Configuration
APP_NAME=copilot-data-science-ml-agent
VERSION=1.0.0
ENVIRONMENT=prod
HOST=0.0.0.0
PORT=9061

# Eureka Configuration (Production)
EUREKA_SERVER=http://service-registry:8761/eureka
EUREKA_INSTANCE_HOST=copilot-data-science-ml-agent

# Google AI Configuration
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_PROJECT_ID=your_project_id
# GOOGLE_LOCATION=us-central1

# Azure Configuration
MODEL_NAME=azure/gpt-4o
AZURE_API_KEY=your_azure_api_key_here
AZURE_API_BASE=https://kognitool-ai-services.openai.azure.com/
AZURE_API_VERSION=2024-02-01

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/database_name
# sqlite:////app/data/db/data_science_agent.db

# ChromaDB Configuration
CHROMA_DB_DIR=/app/data/chroma_db

# Application Settings
BACKEND_URL=http://copilot-data-science-ml-agent:8000
MAX_ITERATIONS=10
TEMPERATURE=0.7
UPLOAD_DIR=/app/data/uploads
MAX_FILE_SIZE_MB=100
SESSION_TIMEOUT_MINUTES=60

# Logging
LOG_LEVEL=info

# AWS S3 Configuration (for trial-based data sync)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# S3 Trial Configuration
S3_BUCKET_NAME=kolate-app-dev
S3_BASE_PATH=bronze/
S3_TEMP_DIR=./data/s3_temp

# S3 Sync Scheduler Configuration
S3_SYNC_ENABLED=true
S3_POLL_INTERVAL_MINUTES=15